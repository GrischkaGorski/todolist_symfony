<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}My TodoList{% endblock %}</title>
        {% block stylesheets %}
                {{ encore_entry_link_tags('app') }}
            {#Todo #}
        {% endblock %}
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}

            <!-- Renders app.ts & a webpack runtime.js file
                <script src="/build/runtime.js" defer></script>
                <script src="/build/app.ts" defer></script>
                See note below about the "defer" attribute -->
        {% endblock %}
    </head>
    <body>
        {% block body %}
            <div id="content">
                {% block content %}{% endblock %}
            </div>
        {% endblock %}
    </body>
</html>
